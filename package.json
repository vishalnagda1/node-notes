{
  "name": "CricketTech",
  "version": "0.0.1",
  "description": "CricketTech",
  "author": "",
  "main": "server.js",
  "private": true,
  "engines": {
    "node": ">=8.9.0",
    "npm": ">=5.5.0",
    "yarn": ">=1.2.1"
  },
  "scripts": {
    "start": "node server.js",
    "start:debug": "cross-env DEBUG=CricketTech:* npm start",
    "lint": "esw *.js server config --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --compilers js:babel-core/register --ui bdd --reporter spec --colors server --recursive",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "cross-env NODE_ENV=test ./node_modules/.bin/istanbul cover _mocha -- --compilers js:babel-core/register --ui bdd --reporter spec --colors server --recursive",
    "test:check-coverage": "npm run test:coverage && istanbul check-coverage",
    "report-coverage": "coveralls < ./coverage/lcov.info",
    "postinstall": "./bin/symLinkHooks.sh",
    "checkGitHooks": "ls -la .git/hooks/ | grep '\\->'",
    "start2": "export NODE_ENV=development;nodemon --legacy-watch -w . --ignore './data/*.json' --exec \"babel-node ./server.js\"",
    "startP": "export NODE_ENV=production;nodemon --legacy-watch -w . --ignore './data/*.json' --exec \"babel-node ./server.js\"",
    "startT": "export NODE_ENV=test;nodemon --legacy-watch -w . --ignore './data/*.json' --exec \"babel-node ./server.js\"",
    "start:debug2": "export NODE_ENV=development;nodemon --inspect --require babel-register src/server.js --presets es2015,stage-0",
    "clean": "sudo rm -rf dist && mkdir dist",
    "build": "babel . -s -D -d dist --presets es2015,stage-0",
    "build:start": "npm run clean; npm run build;npm run start:prod",
    "start:prod": "export NODE_ENV=development;node dist/server.js",
    "eslintFix": "eslint --fix ."
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [],
  "dependencies": {
    "aws-sdk": "2.299.0",
    "bluebird": "3.5.1",
    "body-parser": "1.18.2",
    "compression": "1.7.2",
    "cookie-parser": "1.4.3",
    "cors": "2.8.4",
    "debug": "^2.4.5",
    "dotenv": "^4.0.0",
    "express": "4.16.3",
    "express-jwt": "5.3.1",
    "express-validation": "1.0.2",
    "express-winston": "3.0.0",
    "helmet": "3.12.0",
    "http-status": "1.0.1",
    "joi": "10.6.0",
    "jsonwebtoken": "7.1.9",
    "method-override": "^2.3.10",
    "mongoose": "4.7.4",
    "morgan": "1.9.0",
    "sha1": "1.1.1",
    "supertest": "3.1.0",
    "winston": "3.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.9.0",
    "babel-core": "6.9.0",
    "babel-eslint": "8.2.1",
    "babel-plugin-import": "1.6.3",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.26.0",
    "chai": "4.1.2",
    "commitizen": "^2.9.6",
    "coveralls": "^3.0.0",
    "cross-env": "5.1.4",
    "cz-conventional-changelog": "1.2.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "13.0.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-prettier": "2.6.0",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "3.5.0",
    "nodemon": "1.9.2",
    "prettier": "1.12.1",
    "validate-commit-msg": "^2.14.0"
  },
  "license": "",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
